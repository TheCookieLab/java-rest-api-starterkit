{
 "model": {
    "kind": "model",
    "name": "threadPoolProfile",
    "title": "Thread Pool Profile",
    "description": "To configure thread pools",
    "javaType": "org.apache.camel.model.ThreadPoolProfileDefinition",
    "label": "configuration",
    "input": "false",
    "output": "false"
  },
  "properties": {
    "defaultProfile": { "kind": "attribute", "required": "false", "type": "boolean", "javaType": "java.lang.Boolean", "deprecated": "false", "defaultValue": "false", "description": "Whether this profile is the default thread pool profile" },
    "poolSize": { "kind": "attribute", "required": "false", "type": "string", "javaType": "java.lang.String", "deprecated": "false", "description": "Sets the core pool size" },
    "maxPoolSize": { "kind": "attribute", "required": "false", "type": "string", "javaType": "java.lang.String", "deprecated": "false", "description": "Sets the maximum pool size" },
    "keepAliveTime": { "kind": "attribute", "required": "false", "type": "string", "javaType": "java.lang.String", "deprecated": "false", "description": "Sets the keep alive time for idle threads in the pool" },
    "maxQueueSize": { "kind": "attribute", "required": "false", "type": "string", "javaType": "java.lang.String", "deprecated": "false", "description": "Sets the maximum number of tasks in the work queue. Use -1 or Integer.MAX_VALUE for an unbounded queue" },
